{
  "name": "phoenix",
  "type": "module",
  "version": "1.3.0",
  "engines": {
    "node": ">=18.6.0 <21",
    "npm": ">=9.5.0 <11"
  },
  "engineStrict": true,
  "scripts": {
    "start": "TZ=UTC astro dev",
    "build": "TZ=UTC astro build",
    "build:preview": "npm run build && npm run preview",
    "preview": "TZ=UTC astro preview",
    "test": "TZ=UTC vitest",
    "test:ci": "TZ=UTC vitest --run",
    "check:fmt": "prettier -c \"*.{astro,js,ts,json,md,yml}\"",
    "check:astro": "astro check",
    "check:types": "tsc --noEmit",
    "check:html": "html-validator dist --ignore=\"/^Trailing slash on void elements|^Attribute â€œis:raw/\"",
    "check:all": "concurrently -g --timings \"npm:check:fmt\" \"npm:check:astro\" \"npm:check:types\" \"npm:check:html\"",
    "benchmark": "autocannon -c 100 -d 40 -p 10 http://localhost:4321"
  },
  "dependencies": {
    "@astrojs/mdx": "3.0.1",
    "@astrojs/sitemap": "3.1.5",
    "astro": "4.9.3",
    "lodash": "4.17.21",
    "prismjs": "1.29.0",
    "sharp": "0.33.4"
  },
  "devDependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/ts-plugin": "1.8.0",
    "@csstools/postcss-global-data": "2.1.1",
    "@types/lodash": "4.17.4",
    "autocannon": "7.15.0",
    "concurrently": "8.2.2",
    "dotenv": "16.4.5",
    "husky": "9.0.11",
    "postcss-custom-media": "10.0.6",
    "postcss-nesting": "12.1.5",
    "prettier": "3.3.1",
    "prettier-plugin-astro": "0.14.0",
    "typescript": "5.4.5",
    "vitest": "1.6.0",
    "w3c-html-validator": "1.8.1"
  }
}
